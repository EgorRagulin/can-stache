    "can-log": "^0.1.0",
    "can-namespace": "1.0.0",
    "can-observation": "^4.0.0-pre.7",
    "can-reflect": "^1.2.1",
    "can-stache-key": "^1.0.0-pre.13",
    "can-symbol": "^1.0.0",
    "can-util": "^3.10.6",
    "can-view-callbacks": "^4.0.0-pre.1",
    "can-view-live": "^4.0.0-pre.6",
    "can-view-nodelist": "^3.1.0",
    "can-view-parser": "^4.0.0-pre.0",
