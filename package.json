    "can-vdom": "^3.1.0",
    "detect-cyclic-packages": "^1.1.0",
    "jshint": "^2.9.4",
    "steal": "^1.0.5",
    "steal-benchmark": "^0.0.1",
    "steal-qunit": "^1.0.0",
    "steal-tools": "^1.0.1",
    "testee": "^0.7.0"
  }
